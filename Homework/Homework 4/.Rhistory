set.seed(1)
x <- rnorm(100)
eps <- rnorm(100, sd = sqrt(0.25))
y <- -1 + 0.5 * x + eps
length(y)
plot(x,y)
fit01 <- lm(y ~ x)
summary(fit01)
plot(x,y)
abline(fit01, col = "green")
abline(-1, 0.5, col = "blue")
legend("topleft", c("Least square", "Regression"), col = c("green", "blue"), lty = c(1,1))
fit02 <- lm(y ~ x + I(x^2))
summary(fit02)
set.seed(1)
